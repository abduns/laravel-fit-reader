{
    "name": "abduns/laravel-fit-reader",
    "description": "A focused Laravel 12 package to read Garmin .fit activity files.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dunn",
            "email": "werk@abduns.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/http": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Dunn\\FitReader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dunn\\FitReader\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dunn\\FitReader\\FitReaderServiceProvider"
            ],
            "aliases": {
                "FitReader": "Dunn\\FitReader\\Facades\\FitReader"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
